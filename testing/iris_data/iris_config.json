{
  "label": "IrisTesting",
  "format": "tabular",
  "data_source": "./testing/iris_data/iris_testing.tsv",
  "separator": "\t",
  "foo": "bar",
  "pre_split_hooks": [
    {
      "type": "drop_features_explicit",
      "features": ["color"]
    },
    {
      "type": "feature_drop_null",
      "threshold": 0.5
    }, {
      "type": "sample_drop_null",
      "threshold": 0.5
    }
  ],
  "post_split_hooks": [
    {
      "type": "imputation_simple",
      "strategy": "mean"
    }
  ]
}